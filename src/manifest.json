{
  "name": "GoDictionary",
  "version": "2.3.0",
  "manifest_version": 2,
  "description": "简单快速的在线词典查询扩展，支持有道、必应和爱词霸。",
  "icons": {
    "48": "icon/icon-48.png",
    "64": "icon/icon-64.png"
  },
  "permissions": [
    "contextMenus",
    "https://fanyi.youdao.com/openapi.do",
    "http://dict-co.iciba.com/api/dictionary.php",
    "https://xtk.azurewebsites.net/BingDictService.aspx"
  ],
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "browser_action": {
    "default_title": "GoDictionary",
    "default_popup": "page/main-panel.html",
    "default_icon": {
      "48": "icon/icon-48.png",
      "64": "icon/icon-64.png"
    }
  }
}
